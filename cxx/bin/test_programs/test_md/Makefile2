BIN=test_md
ldflags=
cxxflags=-g -std=c++17 -I$(BOOSTINCLUDE) -I../../../lib/utility
ldlibs=-L../../../lib/utility -lMsPASSutility
SUBDIR=/contrib

include $(ANTELOPEMAKE)  	
include $(ANTELOPEMAKELOCAL)
CXXFLAGS += -I$(BOOSTINCLUDE)

OBJS=test_md.o
$(BIN) : $(OBJS)
	$(RM) $@
	$(CXX) $(CXXFLAGS) -o $@ $(OBJS) $(LDFLAGS) $(LDLIBS)
